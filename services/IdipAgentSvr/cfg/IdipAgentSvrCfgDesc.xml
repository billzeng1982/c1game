<?xml version="1.0" encoding="gb2312" standalone="yes" ?>

<metalib tagsetversion="1" name="IdipAgentSvrCfg" version="1" >
  <struct name="IdipAgentSvrCfg"    version="1"     desc="主配置结构">
    <entry name="ProcIDStr" 	      type="string"   size="20" />
    <entry name="ProcID"		        type="int"      defaultvalue="0"/>
    <entry name="ZoneSvrIDStr" 	    type="string"   size="20" />
    <entry name="RoleSvrIDStr"        type="string"   size="20" />   

    <entry name="ZoneSvrID"		      type="int"      defaultvalue="0" />
    
    <entry name="RoleSvrID"		      type="int"      defaultvalue="0" />

    <entry name="LogQuerySvrIDStr"  type="string"   size="20" />
