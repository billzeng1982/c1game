    <entry name="Level" 			type="uint16" 		desc="主公等级" />
    <entry name="IsPass" 			type="int8" 		desc="是否通关 0-未通关 1-通关 2-首次通关" />
    <entry name="StartTime" 		type="string"   	desc="开始时间"		size="MAX_TIMEINFO_LENGTH" />
    <entry name="EndTime" 			type="string"   	desc="结束时间"		size="MAX_TIMEINFO_LENGTH" />
    <entry name="TimeCost" 			type="uint32" 		desc="关卡通关花费时间,秒" />
    <entry name="MSkillUseNum"		type="int8"			desc="关卡武将技能使用次数" />
    <entry name="PlayerLevel"		type="uint16" 		desc="关卡通关时玩家等级" />
    <entry name="StarEvalResult"	type="uint16" 		desc="关卡通关评星数" />
    <entry name="General1"			type="uint32" 		desc="武将1" />
    <entry name="General2"			type="uint32" 		desc="武将2" />
    <entry name="General3"			type="uint32" 		desc="武将3" />
    <entry name="General4"			type="uint32" 		desc="武将4" />
    <entry name="General5"			type="uint32" 		desc="武将5" />
    <entry name="General6"			type="uint32" 		desc="武将6" />
    <entry name="MSkillID" 			type="uint32" 		desc="军师技ID" />
    <entry name="Time2AccRegister"	type="uint32" 		desc="和与账号注册时间的差值，秒" />
    <entry name="IsAutoBattle"		type="int8"		desc="是否是自动战斗" />
    <entry name="IsAccelerated"		type="int8"		desc="是否已加速" />
    <entry name="MaxFramePerSecond"		type="int"		    desc="最大帧率" />
    <entry name="MinFramePerSecond"		type="int"		    desc="最小帧率" />
    <entry name="AverageFramePerSecond"		type="int"		    desc="平均帧率" />
    <entry name="ReservePara1"		type="uint32" 		desc="预留参数1" />
    <entry name="ReservePara2"		type="uint32" 		desc="预留参数2" />
  </struct>

