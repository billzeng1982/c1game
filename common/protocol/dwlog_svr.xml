<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<metalib tagsetversion="1" name="DWLOG" version="1">

  <include file="dwlog_def.xml" />

  <!--
	data warehouse log, 公司内部数据仓库日志, 由逻辑server记录入库日志的描述结构
	注意日志分流水型日志和统计型日志(极少数特殊情况)
  -->

  <!--统计型日志, 特殊, 入库入查询数据库, replace 语句 !! -->
  <struct name="LevelAccumPassLog" 	version="1" 	desc="关卡累计通关日志, 统计型日志" primarykey='PlayerID'>
    <entry name="PlayerID" 			type="uint64" 		desc="玩家ID" />
    <entry name="AccountName"		type="string" 		desc="玩家账号"		size="MAX_NAME_LENGTH" />
    <entry name="RoleName"			type="string" 		desc="角色名"		size="MAX_NAME_LENGTH" />
    <entry name="Level" 			type="uint16" 		desc="主公等级" />
    <entry name="LevelID" 			type="uint32" 		desc="关卡ID" />
    <entry name="FirstSweepTime"	type="string" 		size="MAX_TIMEINFO_LENGTH" desc="关卡第一次扫荡时间" />
    <entry name="Time2AccRegister"	type="uint32" 		desc="和与账号注册时间的差值，秒" />
    <entry name="AccumPassCnt"  	type="uint32"		desc="关卡累计通关次数, 持续刷新" />
    <entry name="AccumSweepCnt"		type="uint32"		desc="关卡累计扫荡次数, 持续刷新" />
  </struct>

  <struct name="LevelPassLog" version="1" desc="玩家通关流水日志">
